{
  "name": "one23/laravel-translation-yaml",
  "type": "library",
  "homepage": "https://github.com/FlexIDK/laravel-translation-yaml",
  "description": "Support YAML in Laravel translation files",
  "license": "MIT",
  "version": "v0.1.3",
  "authors": [
  ],
  "require": {
    "php" : ">=8",
    "ext-mbstring": "*",
    "ext-yaml": "*",
    "illuminate/translation": "^v9|^v10|^v11"
  },
  "require-dev": {
    "laravel/pint": "^v1",
    "phpstan/phpstan": "^1"
  },
  "autoload": {
    "psr-4": {
      "One23\\LaravelTranslationYaml\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
      ]
    }
  },
  "scripts": {
    "pint": "@php ./vendor/laravel/pint/builds/pint",
    "phpstan": "@php ./vendor/bin/phpstan analyse --memory-limit=2G",
    "code-quality": [
      "@pint",
      "@phpstan"
    ]
  }
}
